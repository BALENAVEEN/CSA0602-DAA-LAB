#include <stdio.h>

// Function to check if a number is already in the result array
int exists(int arr[], int size, int num) {
    for (int i = 0; i < size; i++)
        if (arr[i] == num)
            return 1;
    return 0;
}

// Function to create a list of unique elements
int makeUnique(int arr[], int n, int unique[]) {
    int count = 0;
    for (int i = 0; i < n; i++) {
        if (!exists(unique, count, arr[i])) {
            unique[count++] = arr[i];
        }
    }
    return count;
}

int main() {
    // ---------- Test Case 1 ----------
    int arr1[] = {3, 7, 3, 5, 2, 5, 9, 2};
    int n1 = sizeof(arr1) / sizeof(arr1[0]);
    int unique1[n1];
    int size1 = makeUnique(arr1, n1, unique1);

    printf("Test Case 1 Output: [");
    for (int i = 0; i < size1; i++)
        printf("%d%s", unique1[i], (i < size1 - 1) ? ", " : "");
    printf("]\n");

    // ---------- Test Case 2 ----------
    int arr2[] = {-1, 2, -1, 3, 2, -2};
    int n2 = sizeof(arr2) / sizeof(arr2[0]);
    int unique2[n2];
    int size2 = makeUnique(arr2, n2, unique2);

    printf("Test Case 2 Output: [");
    for (int i = 0; i < size2; i++)
        printf("%d%s", unique2[i], (i < size2 - 1) ? ", " : "");
    printf("]\n");

    // ---------- Test Case 3 ----------
    int arr3[] = {1000000, 999999, 1000000};
    int n3 = sizeof(arr3) / sizeof(arr3[0]);
    int unique3[n3];
    int size3 = makeUnique(arr3, n3, unique3);

    printf("Test Case 3 Output: [");
    for (int i = 0; i < size3; i++)
        printf("%d%s", unique3[i], (i < size3 - 1) ? ", " : "");
    printf("]\n");

    return 0;
}
