#include <stdio.h>

// Function to find maximum element in array
int findMax(int arr[], int n) {
    int max = arr[0];
    for (int i = 1; i < n; i++) {
        if (arr[i] > max)
            max = arr[i];
    }
    return max;
}

int main() {
    int arr1[] = {1, 2, 3, 4, 5};
    int arr2[] = {7, 7, 7, 7, 7};
    int arr3[] = {-10, 2, 3, -4, 5};

    int n1 = sizeof(arr1) / sizeof(arr1[0]);
    int n2 = sizeof(arr2) / sizeof(arr2[0]);
    int n3 = sizeof(arr3) / sizeof(arr3[0]);

    printf("Test Case 1 Output: %d\n", findMax(arr1, n1));
    printf("Test Case 2 Output: %d\n", findMax(arr2, n2));
    printf("Test Case 3 Output: %d\n", findMax(arr3, n3));

    return 0;
}
