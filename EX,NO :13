#include <stdio.h>

// Function to calculate number of distinct ways
int climbStairs(int n) {
    if (n == 0 || n == 1)
        return 1;
    
    int prev2 = 1; // ways(0)
    int prev1 = 1; // ways(1)
    int current;

    for (int i = 2; i <= n; i++) {
        current = prev1 + prev2;
        prev2 = prev1;
        prev1 = current;
    }

    return prev1;
}

int main() {
    int n1 = 4, n2 = 3;
    
    printf("Input: n = %d\nOutput: %d\n\n", n1, climbStairs(n1));
    printf("Input: n = %d\nOutput: %d\n", n2, climbStairs(n2));

    return 0;
}
