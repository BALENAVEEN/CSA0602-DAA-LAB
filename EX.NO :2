#include <stdio.h>

int main() {
    int nums1[] = {4, 3, 2, 3, 1};
    int nums2[] = {2, 2, 5, 2, 3, 6};
    int n = sizeof(nums1) / sizeof(nums1[0]);
    int m = sizeof(nums2) / sizeof(nums2[0]);
    
    int answer1 = 0, answer2 = 0;

    // Calculate answer1
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (nums1[i] == nums2[j]) {
                answer1++;
                break; // Move to next nums1[i]
            }
        }
    }

    // Calculate answer2
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            if (nums2[i] == nums1[j]) {
                answer2++;
                break; // Move to next nums2[i]
            }
        }
    }

    printf("Output: [%d, %d]\n", answer1, answer2);

    return 0;
}
